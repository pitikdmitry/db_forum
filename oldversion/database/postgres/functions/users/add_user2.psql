CREATE OR REPLACE FUNCTION add_user2(
  p_nickname CITEXT,
  p_email CITEXT,
  p_about TEXT,
  p_fullname TEXT)

RETURNS TABLE(user_id INT)
AS
$BODY$
  BEGIN
    RETURN QUERY
        INSERT INTO users (nickname, email, about, fullname)
        VALUES (p_nickname, p_email, p_about, p_fullname)
        RETURNING users.user_id;
  END;
$BODY$
LANGUAGE plpgsql VOLATILE;